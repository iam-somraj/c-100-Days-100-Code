#include <stdbool.h>
#include <stdio.h>
#include <string.h>

void SieveOfEratosthenes(int srt, int n)
{

    bool prime[n + 1];
    memset(prime, true, sizeof(prime));
    prime[0] = false;
    prime[1] = false;

    for (int p = 2; p * p <= n; p++) {
        if (prime[p] == true) {
            for (int i = p * p; i <= n; i += p)
                prime[i] = false;
        }
    }

    for (int p = srt; p <= n; p++)
        if (prime[p])
            printf("%d ", p);
}

int main()
{
    int srt = 1;
    int end = 10;
    SieveOfEratosthenes(srt, end);
    return 0;
}
