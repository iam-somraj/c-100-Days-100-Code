#include <stdio.h>

float fahrenheit_to_celsius(float f)
{
    return ((f - 32.0) * 5.0 / 9.0);
}

int main()
{
    float f;

    printf("Enter the temperature in Fahrenheit: ");
    scanf("%f", &f);

    printf("Temperature in Degree Celsius: %.2f\n",
           fahrenheit_to_celsius(f));

    return 0;
}
