#include <stdio.h>
#include <string.h>

int isPalindrome(int n) {
    char str[20];
  
    sprintf(str, "%d", n);

    int left = 0;   
  
    int right = strlen(str) - 1;

    while (left < right) {
      
        if (str[left] != str[right]) {
            return 0;
        }
      	
        left++;
        right--;
    }

    return 1;
}

int main() {
    int num = 1221;

    if (isPalindrome(num)) {
        printf("Yes\n");
    }
    else {
        printf("No\n");
    }

    return 0;
}
