#include <stdio.h>
#include <math.h>

int isArmstrong(int num) {
    int originalNum, remainder, result = 0;
    int n;

    originalNum = num;  
    n = log10(num) + 1;  

    while (originalNum != 0) {
        remainder = originalNum % 10;
        result += pow(remainder, n);
        originalNum /= 10;
    }
    return (result == num);
}

int main() {
    int low = 1, high = 1000; 

    printf("Armstrong numbers between %d and %d are: ", low, high);

    for (int i = low + 1; i < high; ++i) {
        if (isArmstrong(i)) {
            printf("%d ", i);
        }
    }

    return 0;
}
