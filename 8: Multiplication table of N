#include <stdio.h>

void main()
{
    int n,l;
    printf("Enter the number: ");
    scanf("%d", &n);

    printf("Enter the limit: ");
    scanf("%d", &l);
    
    printf("Multiplication table of %d:\n", n);

    for (int i = 1; i <= l; i++)
    {
        printf("%d x %d = %d\n", n, i, n * i);
    }
}
