#include <stdio.h>

void main()
{
    int n, i;
    float fact = 1;

    printf("Enter a number: ");
    scanf("%d", &n);

    if (n < 0)
        printf("Factorial of a negative number doesn't exist\n");
    else
    {
        for (i = 1; i <= n; i++)
        {
            fact = fact * i;
        }
        printf("Factorial of %d = %.0f\n", n, fact);
    }
}
