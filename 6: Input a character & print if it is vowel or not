#include <stdio.h>
#include <ctype.h>

void main()
{
    char ch;
    printf("Enter a character: ");
    scanf("%c", &ch);
    
    if(isalpha(ch))
    {
        ch=tolower(ch);
    }
    
    if(ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u')
    {
        printf("The alphabet is Vowel");
    }
    else
    {
        printf("The alphabet is Consonent");
    }
}
