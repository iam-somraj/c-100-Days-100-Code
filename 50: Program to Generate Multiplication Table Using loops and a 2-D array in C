#include <stdio.h>
void print_table(int range, int num)
{
    int row, col;

    row = range;

    col = 3;

    int arr[row][col];

for (int k = 0; k < row; k++) {
        arr[k][0] = num;

        arr[k][1] = k + 1;

        arr[k][2] = arr[k][1] * arr[k][0];
    }

    for (int i = 0; i < row; i++) {
        printf("%d * %d = %d", arr[i][0], arr[i][1],
               arr[i][2]);
        printf("\n");
    }
}

int main()
{

    int range = 10;

    int num = 5;

    print_table(range, num);
    return 0;
}
